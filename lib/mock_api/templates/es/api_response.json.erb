{
  "took" : 0,
  "timed_out" : false,
  "_shards" : {
    "total" : 1,
    "successful" : 1,
    "failed" : 0
  },
  "hits" : {
    "total" : "<%= @records.count.to_s %>",
    "max_score" : null,
    "hits" : [
